json.group do
	json.name 		@group.name
	json.user_id	@group.id
	json.website	@group.url
	json.location @group.address
	json.logo			@group.logo.url
	json.tags do
		@group.tags.each do |t|
			json.name t.name.gsub(/\r\n/, " ")
		end
	end
	json.employees do
		@group.employees.each do |e|
			json.name e.name
		end
	end
end

puts json.compile!